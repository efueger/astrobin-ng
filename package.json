{
  "name": "astrobin-ng",
  "version": "0.1.0",
  "description": "AstroBin NG",
  "license": "AGPL-3.0",
  "repository": "https://github.com/astrobin/astrobin-ng",
  "scripts": {
    "backend:lint": "tslint -p backend/tsconfig.json -c backend/tslint.json",
    "backend:build": "tsc -p backend/tsconfig.build.json",
    "backend:start": "ts-node -P backend/tsconfig.json -r tsconfig-paths/register backend/src/main.ts",
    "backend:start:dev": "concurrently --handle-input \"wait-on backend/dist/backend/src/main.js && nodemon\" \"tsc -w -p backend/tsconfig.build.json\" ",
    "backend:start:debug": "nodemon --config backend/nodemon-debug.json",
    "backend:prestart:prod": "rimraf backend/dist && npm run backend:build",
    "backend:start:prod": "node backend/dist/backend/src/main.js",
    "backend:test": "jest --config backend/jest.config.js",
    "backend:test:watch": "jest --config backend/jest.config.js --watch",
    "backend:test:cov": "jest --config backend/jest.config.js --coverage",
    "backend:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config backend/jest.config.js --runInBand",
    "backend:test:e2e": "jest --config backend/jest.config.js --config backend/test/jest-e2e.json",
    "ng": ".node_modules/.bin/ng",
    "frontend:lint": "cd frontend; ng lint",
    "frontend:start": "cd frontend; ng serve --host 0.0.0.0 --port 4400",
    "frontend:start:prod": "cd frontend; ng serve --host 0.0.0.0 --port 4400 --configuration=local-production",
    "frontend:build": "cd frontend; ng build",
    "frontend:build:prod": "cd frontend; ng build --prod --aot",
    "frontend:test": "jest --config frontend/jest.config.js",
    "frontend:test:cov": "jest --config frontend/jest.config.js --coverage",
    "frontend:test:e2e": "jest --config frontend/jest.config.js --config frontend/test/jest-e2e.json",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular/animations": "^8.2.8",
    "@angular/common": "8.2.10",
    "@angular/compiler": "8.2.10",
    "@angular/core": "8.2.10",
    "@angular/forms": "^8.2.8",
    "@angular/platform-browser": "8.2.10",
    "@angular/platform-browser-dynamic": "8.2.10",
    "@angular/router": "8.2.10",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@nestjs/common": "6.8.3",
    "@nestjs/core": "6.8.3",
    "@nestjs/elasticsearch": "^6.0.1",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/microservices": "6.8.3",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.8.3",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/typeorm": "6.2.0",
    "@nestjs/websockets": "6.8.3",
    "@nestjsx/crud": "4.2.0",
    "@nestjsx/crud-typeorm": "4.2.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@ngx-formly/bootstrap": "^5.4.3",
    "@ngx-formly/core": "^5.4.3",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/elasticsearch": "^5.0.35",
    "bootstrap": "4.3.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.2",
    "core-js": "^3.2.1",
    "dotenv": "8.1.0",
    "elasticsearch": "^16.3.0",
    "express-basic-auth": "1.2.0",
    "intl": "1.2.5",
    "jwt-decode": "^2.2.0",
    "ngx-toastr": "^11.0.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.5.3",
    "swagger-ui-express": "4.1.2",
    "tslib": "^1.10.0",
    "typeorm": "0.2.19",
    "webpack-bundle-tracker": "^0.4.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.9",
    "@angular/cli": "8.3.9",
    "@angular/compiler-cli": "8.2.10",
    "@angular/language-service": "8.2.10",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@nestjs/testing": "6.8.3",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.7",
    "codelyzer": "^5.0.1",
    "concurrently": "5.0.0",
    "faker": "4.1.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^7.1.1",
    "ng-mocks": "^8.1.0",
    "nodemon": "1.19.3",
    "prettier": "1.17.0",
    "protractor": "^5.4.2",
    "sqlite3": "4.1.0",
    "supertest": "4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.4.5",
    "wait-on": "3.3.0"
  },
  "jest": {
    "projects": [
      "backend/jest.config.js",
      "frontend/jest.config.js"
    ]
  }
}
